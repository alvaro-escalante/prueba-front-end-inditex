    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.1.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "undici": "^6.18.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
